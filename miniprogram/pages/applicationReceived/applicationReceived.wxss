.stringChinese {
  position: absolute;
  width: 34.87vw;
  height: 4.15vh;
  left: 6.15vw;
  top: 6.75vh;

  /* font-family: PingFang SC; */
  font-style: normal;
  font-weight: 500;
  font-size: 2.96vh;
